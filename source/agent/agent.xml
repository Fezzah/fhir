    <Cell><Data ss:Type="String">1..1</Data></Cell>
    <Cell><Data ss:Type="String">Mandatory</Data></Cell>
    <Cell><Data ss:Type="String">Narrative</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String">Text summary of the agent, for human interpretation</Data></Cell>
