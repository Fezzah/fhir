   <Row ss:Height="72">
    <Cell><Data ss:Type="String">DeviceCapabilities</Data></Cell>
    <Cell ss:StyleID="s65"/>
    <Cell ss:StyleID="s65"/>
    <Cell ss:StyleID="s65"/>
    <Cell><Data ss:Type="String">Resource</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String"> A set of data produced by a device</Data></Cell>
    <Cell><Data ss:Type="String"> A set of data produced by a device</Data></Cell>
    <Cell><Data ss:Type="String">A low level ultra-simple resource is needed to devices that send current observations on a routine bases as an event log</Data></Cell>
    <Cell><Data ss:Type="String">This resource can only be understood in the context of the DeviceCapabilities that defines what the meanings of the key values are</Data></Cell>
   </Row>
   <Row ss:Height="28.8">
    <Cell><Data ss:Type="String">DeviceCapabilities.name</Data></Cell>
    <Cell><Data ss:Type="String">0..1</Data></Cell>
