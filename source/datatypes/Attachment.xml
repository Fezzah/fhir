   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">Must Have Data</Data></Cell>
    <Cell ss:StyleID="Default"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">Unless the Attachment element has a dataAbsentReason flag, it must contain a </Font><I><Font
        html:Color="#000000">data</Font></I><Font html:Color="#000000"> or a </Font><I><Font
        html:Color="#000000">url</Font></I></ss:Data></Cell>
    <Cell><Data ss:Type="String">dataAbsentReason.oclIsDefined or data.size &gt; 0 or url.size &gt; 0</Data></Cell>
    <Cell><Data ss:Type="String" x:Ticked="1">@dataAbsentReason or data or url</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">data Must contain at least one byte</Data></Cell>
    <Cell ss:StyleID="Default"><Data ss:Type="String">If a data element is provided, at least one byte must be represented</Data></Cell>
    <Cell><Data ss:Type="String">data.oclIsDefined implies data.size &gt; 0</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="28.5">
    <Cell><Data ss:Type="String">URL must point to data</Data></Cell>
    <Cell><Data ss:Type="String">If a URL is provided, it must resolve to actual data. </Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">url Must contain at least one char</Data></Cell>
    <Cell ss:StyleID="Default"><Data ss:Type="String">If a url element is provided, it must be least one character long</Data></Cell>
    <Cell><Data ss:Type="String">url.oclIsDefined implies url.size &gt; 0</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">hash Must contain at least one char</Data></Cell>
    <Cell ss:StyleID="Default"><Data ss:Type="String">If a hash element is provided, it must be least one character long</Data></Cell>
    <Cell><Data ss:Type="String">hash.oclIsDefined implies hash.size &gt; 0</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">lang Must contain at least one char</Data></Cell>
    <Cell ss:StyleID="Default"><Data ss:Type="String">If a lang element is provided, it must be least one character long</Data></Cell>
    <Cell><Data ss:Type="String">lang.oclIsDefined implies lang.size &gt; 0</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">title Must contain at least one char</Data></Cell>
    <Cell ss:StyleID="Default"><Data ss:Type="String">If a title element is provided, it must be least one character long</Data></Cell>
    <Cell><Data ss:Type="String">title.oclIsDefined implies title.size &gt; 0</Data></Cell>
