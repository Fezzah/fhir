 <Worksheet ss:Name="String Patterns">
  <Names>
   <NamedRange ss:Name="code" ss:RefersTo="='String Patterns'!R2C1"/>
   <NamedRange ss:Name="date" ss:RefersTo="='String Patterns'!R7C1"/>
