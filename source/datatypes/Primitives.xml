   <NamedRange ss:Name="uuid" ss:RefersTo="='String Patterns'!R4C1"/>
  </Names>
  <Table ss:ExpandedColumnCount="5" ss:ExpandedRowCount="8" x:FullColumns="1"
   x:FullRows="1" ss:StyleID="s62" ss:DefaultRowHeight="15">
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="74.25"/>
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="233.25"/>
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="142.5"/>
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="199.5"/>
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="108"/>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">Data Type</Data></Cell>
    <Cell><Data ss:Type="String">Definition</Data></Cell>
    <Cell><Data ss:Type="String">Comments</Data></Cell>
    <Cell><Data ss:Type="String">RegEx</Data></Cell>
    <Cell><Data ss:Type="String">Regex Comments</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell><Data ss:Type="String">code</Data><NamedCell ss:Name="code"/></Cell>
    <Cell><Data ss:Type="String">A string which has at least one character and no leading or trailing whitespace</Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">[^\s]+([\s]+[^\s]+)*</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="14.25">
    <Cell><Data ss:Type="String">oid</Data><NamedCell ss:Name="oid"/></Cell>
    <Cell><Data ss:Type="String">An ISO oid</Data></Cell>
    <Cell><Data ss:Type="String">See ISO/IEC 8824:1990 â‚¬</Data></Cell>
    <Cell><Data ss:Type="String">([1-9][0-9]*)(\.[1-9][0-9]*)*</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="47.25">
    <Cell><Data ss:Type="String">uuid</Data><NamedCell ss:Name="uuid"/></Cell>
    <Cell><Data ss:Type="String">A UUID</Data></Cell>
    <Cell><Data ss:Type="String">See The Open Group, CDE 1.1 Remote Procedure Call specification, Appendix A.</Data></Cell>
    <Cell><Data ss:Type="String">[0-9a-zA-Z]{8}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{12}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell><Data ss:Type="String">sid</Data><NamedCell ss:Name="sid"/></Cell>
    <Cell><Data ss:Type="String">A system id, which is a uri taken from the list of known definition systems</Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">[A-Za-z]+:[^s]*</Data></Cell>
    <Cell><Data ss:Type="String">Could be tighter but it gets complicated</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="75">
    <Cell><Data ss:Type="String">id</Data><NamedCell ss:Name="id"/></Cell>
    <Cell><ss:Data ss:Type="String" xmlns="http://www.w3.org/TR/REC-html40"><Font
       html:Color="#000000">A whole number in the range 0 to 2^64-1, optionally represented in hex, a uuid, an oid, or any other combination of </Font><B><Font
        html:Color="#000000">lower-case letters a-z</Font></B><Font
       html:Color="#000000">, numerals, &quot;-&quot; and &quot;.&quot;, with a length limit of 36 characters</Font></ss:Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">[a-z0-9\-\.]{1,36}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="59.25">
    <Cell><Data ss:Type="String">date</Data><NamedCell ss:Name="date"/></Cell>
    <Cell><ss:Data ss:Type="String" xmlns="http://www.w3.org/TR/REC-html40"><Font
       html:Color="#000000">A date, or partial date (e.g. just year or year + month). There is no timezone. The format is a union of the schema types gYear, gYearMonth, and date.  </Font><B><Font
        html:Color="#000000">Dates must be valid dates.</Font></B></ss:Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">[0-9]{4}(\-((0[1-9])|(1[0-2]))(\-((0[1-9])|([1-2][0-9])|(3[0-1])))?)?</Data></Cell>
    <Cell><Data ss:Type="String">Could get fancy and check legal days/month or even leap years, but not worth it.</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="109.5">
    <Cell><Data ss:Type="String">dateTime</Data><NamedCell ss:Name="dateTime"/></Cell>
    <Cell><ss:Data ss:Type="String" xmlns="http://www.w3.org/TR/REC-html40"><Font
       html:Color="#000000">A date, date-time or partial date (e.g. just year or year + month).  If hours and minutes are specified, a timezone must be populated. The format is a union of the schema types gYear, gYearMonth, date, and dateTime. Seconds may be provided but may also be ignored.  </Font><B><Font
        html:Color="#000000">Dates must be valid dates.</Font></B></ss:Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">[0-9]{4}(\-((0[1-9])|(1[0-2]))(\-((0[1-9])|([1-2][0-9])|(3[0-1]))(T(([0-1][0-9])|(2[0-3])):[0-5][0-9]:[0-5][0-9]((\+|-)(0[0-9]|1[0-4]):(0|3|4)(0|5)|Z)?)?)?)?</Data></Cell>
   </Row>
  </Table>
