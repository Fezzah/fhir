    <Cell ss:Index="5"><Data ss:Type="String">Narrative</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String">Text summary of message, for human interpretation</Data></Cell>
    <Cell><Data ss:Type="String"> </Data></Cell>
   </Row>
