    <Cell><Data ss:Type="String">Mandatory</Data></Cell>
    <Cell><Data ss:Type="String">string</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String">Informal name for this profile</Data></Cell>
    <Cell><Data ss:Type="String">A free text natural language name identifying the Template.</Data></Cell>
    <Cell ss:Index="10"><Data ss:Type="String">Not expected to be unique</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell><Data ss:Type="String">Profile.author</Data></Cell>
    <Cell><Data ss:Type="String">1..1</Data></Cell>
    <Cell><Data ss:Type="String">Mandatory</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String">(Organisation or individual)</Data></Cell>
    <Cell><Data ss:Type="String">Details of the author who accepts responsibility for publishing the profile</Data></Cell>
    <Cell ss:Index="10"><Data ss:Type="String">Usually an organisation, but may be an individual</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell><Data ss:Type="String">Profile.author.name</Data></Cell>
